
// fn doThing() int extern;

fn main() export {
  if (numMainArgs() < 4) {
    println("Usage: " + getMainArg(0) + " input.vmd output.html Valestrom.jar");
    ret void();
  }

  inFilepath = getMainArg(1);
  outFilepath = getMainArg(2);
  valestromPath = getMainArg(3);

  page = parse(inFilepath);

  output = Output();
  printPage(&output, valestromPath, &page);

  writeStringToFile(outFilepath, output.assembleStr());
}
